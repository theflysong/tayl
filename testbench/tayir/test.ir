def @main(i32 %argc, p64 %argv) -> i32 {
start:
    // because we use scanf, so we use instruction `alloc` to alloc space from stack
    %a$ptr = alloc i32
    %b$ptr = alloc i32
    call @scanf, ["%d%d", %a, %b]
    %a$val = load %a$ptr
    %b$val = load %b$ptr
    %temp$0 = mul %b$val, 2
    %c = add %a, %temp$0
    call @printf, ["%d\n", %c]
    ret 0
}